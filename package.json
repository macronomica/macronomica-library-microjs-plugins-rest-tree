{
  "name": "@microjs/microjs-plugins-rest-tree",
  "version": "0.1.0",
  "description": "Плагин Microjs для запросов к древовидным сущностям",
  "main": "index.js",
  "author": "Konstantin Dark",
  "scripts": {
    "dev:setup": "npm i && npm run dev:build",
    "dev:build": "npm run clean:build && node_modules/.bin/gulp build",
    "dev:watch": "node_modules/.bin/gulp watch",
    "prod:build": "npm run clean:release && node_modules/.bin/gulp build:release",
    "clean:build": "node_modules/.bin/rimraf ./build",
    "clean:release": "node_modules/.bin/rimraf ./release",
    "clean:modules": "node_modules/.bin/rimraf ./node_modules",
    "git:patch": "node_modules/.bin/gulp patch",
    "git:feature": "node_modules/.bin/gulp feature",
    "git:release": "node_modules/.bin/gulp release",
    "test": "./node_modules/.bin/mocha -R spec --timeout 5000 test/index.js test/**/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/microjs/microjs-plugins-rest-tree.git"
  },
  "dependencies": {},
  "peerDependencies": {
    "microjs": "gitlab:microjs/microjs",
    "microjs-plugins-dal": "gitlab:microjs/microjs-plugins-dal"
  },
  "devDependencies": {
    "babel-eslint": "^7.0.0",
    "babel-plugin-transform-es2015-destructuring": "^6.9.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "eslint": "^3.7.1",
    "eslint-plugin-react": "^6.4.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-bump": "^2.1.0",
    "gulp-git": "^1.7.2",
    "gulp-sourcemaps": "2.0.1",
    "gulp-tag-version": "^1.3.0",
    "mocha": "^3.1.2",
    "semver": "^5.3.0",
    "rimraf": "^2.5.4"
  }
}
